/* pages/brand/brand.wxss */
.brand-head {
  width: 100%;
  height: 150px;
  background-color: burlywood;
}
.brand-tab {
  width: 100%;
  border-bottom: 2rpx solid #ccc;
  text-align: center;
  height: 60px;
  line-height: 60px;
  display: flex;
  flex-flow: row;
  justify-content: space-between;
}
.brand-tab-item {
  width: 30%; 
  font-size: 16px;
}
.active{
  color:rgb(54, 156, 114);
  border-bottom: 4rpx solid rgb(37, 146, 56);
}